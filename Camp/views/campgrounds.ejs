<%include partials/header%>
<div class="container">
    <header class="jumbotron">
        <div class="container">
            
        
            <h1>Welcome to YelpCamp!</h1>
            <p>View it all over the world</p>
            <p>
                <a href="/campgrounds/new" class="btn btn-primary btn-large">Add New Campground</a>
            </p>
        </div>
    </header>


    <div class="row">
        <% arr.forEach(function(whale){%>
            <div class="col-md-3 col-sm-6">


                <div class="thumbnail">
                    <img src="<%= whale.image %>">
                    <div class="caption">
                        <h4><%= whale.name%></h4>
                    </div>
                    
                </div>
                
            </div>

        <% })  %>
    </div>

</div>
<%include partials/footer%>